
{{#extend 'layouts/dashboard/simple' title="password-recover" active="auth-recoverpw" path="../../" }}
    {{#content "content"}}
   <div class="robot">
      <div class="container h-100">
         <div class="row justify-content-center h-100 align-items-center">
            <div class="col-lg-5 col-md-8">   
               <div class="card" style="max-width: 470px; max-height: 502px;">
                  <div class="card-header d-flex justify-content-center" >
                     <a href="{{path}}dashboard/index.html" class="navbar-brand d-flex align-items-center">
                        {{> partials/components/logo }}
                     </a>
                  </div>
                  <div class="card-body">
                     <h5 class="text-center mb-4">Change password</h5>
                     <form>
                        <div class="row">
                           <div class="col-lg-12">
                              <div class="form-group">
                                 <input type="text" class="form-control" id="old-password" placeholder="Old Password">
                              </div>
                           </div>
                           <div class="col-lg-12">
                              <div class="form-group">
                                 <input type="password" class="form-control active" id="new-password" placeholder="New password">
                              </div>
                           </div>
                           <div class="col-lg-12">
                              <div class="form-group">
                                 <input type="text" class="form-control" id="confirm-password" placeholder="confirm-password">
                              </div>
                           </div>
                        </div>
                        <div class="d-flex justify-content-center">
                           <button type="submit" class="btn btn-primary text-white">Change Password</button>
                        </div>
                     </form>
                  </div>
               </div>    
            </div>
         </div>           
      </div>
      <div class="auth-img">
         <div class="cp-bottom-image">
            <svg width="250" height="250" viewBox="0 0 524 519" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
               <rect y="148.456" width="512" height="512" transform="rotate(-16.8552 0 148.456)" fill="url(#pattern0)" fill-opacity="0.8"/>
               <defs>
               <pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
               <use xlink:href="#image0" transform="scale(0.00195312)"/>
               </pattern>
               <image id="image0" data-name="shield.png" width="512" height="512" xlink:href="data:image/png;base64,"/>
               </defs>
            </svg>
         </div>
         <img src="{{path}}assets/images/auth/06.png" alt="user" class="cp-left-img w-25">
      </div>
   </div> 
              
      
        
      
   {{/content}}
{{/extend}}